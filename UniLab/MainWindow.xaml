<Window x:Class="UniLab.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UniLab"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Publicación1.jpg"/>
        </Grid.Background>
        <Button x:Name="LogInBtn" Click="LogInBtn_Click" Content="Iniciar Sesion" HorizontalAlignment="Left" VerticalAlignment="Top" Width="140" Margin="310,226,0,0" Height="24"/>
        <Button x:Name="RegistrateBtn" Content="Registrarse" HorizontalAlignment="Left" VerticalAlignment="Top" Width="140" Margin="310,255,0,0" Height="24"/>

        <TextBox x:Name="UserNameTxtB" HorizontalAlignment="Left" Height="19" Margin="310,139,0,0" TextWrapping="Wrap" Text="Nombre de usuario" VerticalAlignment="Top" Width="140" RenderTransformOrigin="1.988,0.548" Background="{DynamicResource {x:Static SystemColors.ActiveCaptionBrushKey}}"/>
        <TextBox x:Name="PasswordTxtB" HorizontalAlignment="Left" Height="21" Margin="310,163,0,0" TextWrapping="Wrap" Text="Clave" VerticalAlignment="Top" Width="140" Background="{DynamicResource {x:Static SystemColors.ActiveCaptionBrushKey}}"/>

    </Grid>
</Window>
